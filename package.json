{
  "name": "express-example-2019",
  "version": "1.0.0",
  "description": "My cool TypeScript app",
  "main": "src/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "lint": "tslint --exclude **/*.d.ts --project . --fix src/**/*.ts"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "pino": "^5.8.1",
    "swagger-express-middleware": "^1.1.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^24.0.5",
    "@types/pino": "^5.20.0",
    "@types/supertest": "^2.0.7",
    "delay": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "pino-pretty": "^2.5.0",
    "prettier": "^1.16.4",
    "shelljs": "^0.8.3",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2"
  },
  "author": "piisuke2 <piisuke2@gmail.com> (https://github.com/piisuke2)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/piisuke2/express-example-2019.git"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.ts"
  }
}
